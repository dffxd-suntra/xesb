{
    "manifest_version": 2,
    "name": "xesb",
    "description": "给e-hentai或exhentai提供更好的体验",
    "version": "3.0.1",
    "version_name": "3.0.1 仅适配下载功能",
    "author": "Suntra",
    "options_page": "index.html?tab=options",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "icons/icon-x16.png",
        "32": "icons/icon-x32.png",
        "64": "icons/icon-x64.png",
        "128": "icons/icon-x128.png",
        "256": "icons/icon-x256.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "点击查看更多"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "unlimitedStorage",
        "downloads",
        "tabs",
        "activeTab",
        "*://*/*"
    ],
    "background": {
        "page": "background.html",
        "persistent": true
    }
}